#include<stdio.h>

void alphacheck(int ch)
{
    if(ch>=65 && ch<=122)
    {
        printf("\nGiven input is alphabet!");
    }
    else{
        printf("\nGiven input is not alphabet.");
    }
}

int main()
{
    char ch;
    printf("\nEnter a character to check :-");
    scanf("%c",&ch);
    alphacheck(ch);

    return 0;
}
