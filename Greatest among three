#include<stdio.h>

int greatestamong(int a,int b,int c)
{
    if(a>b && a>c)
      return a;
    else if(b>a && b>c)
        return b;
    else
       return c;
}

int main()
{
    int x,y,z;
    printf("\nEnter three number to compare :-");
    scanf("%d%d%d",&x,&y,&z);
    int greatest;
    greatest = greatestamong(x,y,z);
    printf("\nGreatest among %d ,%d & %d is %d",x,y,z,greatest);

    return 0;
}
