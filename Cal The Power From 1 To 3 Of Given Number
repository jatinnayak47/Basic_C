#include<stdio.h>
#include<math.h>

void powdisp(int num)
{
    for(int i = 1; i<=3;i++)
    {
        int poW =pow(num,i);
        printf("%d pow %d is %d\n",num,i,poW);
    }
}

int main()
{
    int num;
    printf("\nEnter Number :-");
    scanf("%d",&num);
    printf("\n\n");
    powdisp(num);

    return 0;
}
