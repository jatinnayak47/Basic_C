#include<stdio.h>

void check(char ch)
{
    if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
    {
        printf("\nGiven character is vowel.");
    }
    else
    {
        printf("\nGiven character is consonant.");
    }
}

int main()
{
    char c;
    printf("\nEnter a character to check :-");
    scanf("%c",&c);
    check(c);

    return 0;
}
